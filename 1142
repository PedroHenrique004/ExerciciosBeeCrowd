#include <stdio.h>

int main(){
    int limite, valorInicial;
    valorInicial = 1;
    scanf("%d", &limite);

    for (int i = 0; i < limite; i++){
            printf("%d %d %d PUM\n", 
                valorInicial,
                valorInicial + 1,
                valorInicial + 2);
    valorInicial = valorInicial + 4;

    }


    return 0;
}
