#include <stdio.h>


int main (){
    int chaCerto, chaResposta, respostaCertas;
    respostaCertas = 0;

    scanf("%d", &chaCerto); 

    for (int i = 0; i < 5; i++){
        scanf("%d", &chaResposta);
        if (chaResposta == chaCerto){
            respostaCertas++;
        }
    }

    printf("%d\n", respostaCertas);
    return 0;
    
    }
