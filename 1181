#include <stdio.h>

int main (){

    int linhaOpe;
    char operacao;

    double matriz[12][12];

    scanf("%d", &linhaOpe);
    scanf(" %c", &operacao);

    for (int linha  = 0;linha < 12, linha++){
        for (int coluna = 0; coluna < 12; coluna++){
            scanf("%lf", &matriz[linha][coluna]);
        }
    }

    for (int coluna = 0; coluna < 12; coluna ++){
        soma = soma + matriz[linhaOpe][coluna];
    }

    if (operacao == 'S'){
        printf("%.1lf\n", soma;)
    }

    else if (operacao == 'M'){
        printf("%.1lf\n", soma / 12);
    }

    return 0;
}
